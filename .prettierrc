{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "trailingComma": "all",
  "singleQuote": true,
  "arrowParens": "always",
  "jsxBracketSameLine": false,
  "semi": true,
  "tabWidth": 2,
  "bracketSpacing": true,
  "importOrder": ["builtin", "<THIRD_PARTY_MODULES>", "^@/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
